{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$ref": "#/definitions/Addresses",
  "definitions": {
    "Addresses": {
      "type": "object",
      "properties": {
        "meta": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            },
            "party_id": {
              "type": "string"
            },
            "total_records": {
              "type": "integer"
            },
            "page": {
              "type": "integer"
            },
            "per_page": {
              "type": "integer"
            }
          },
          "required": [
            "type",
            "party_id",
            "total_records",
            "page",
            "per_page"
          ]
        },
        "data": {
          "type": "array",
          "items": {
            "$ref": "https://raw.githubusercontent.com/indiaprotocol/protocol/master/v1/party/PartyAddress.json"
          }
        }
      },
      "required": [
        "meta",
        "data"
      ]
    }
  }
}