{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$ref": "#/definitions/Documents",
  "definitions": {
    "Documents": {
      "type": "object",
      "required": [
        "meta",
        "data"
      ],
      "properties": {
        "meta": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            },
            "party_id": {
              "type": "string"
            },
            "total_records": {
              "type": "integer"
            },
            "page": {
              "type": "integer"
            },
            "per_page": {
              "type": "integer"
            }
          },
          "required": [
            "type",
            "party_id",
            "total_records",
            "page",
            "per_page"
          ]
        },
        "data": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "document_id",
              "party_id",
              "type",
              "date",
              "ref",
              "name",
              "category",
              "sources"
            ],
            "additionalProperties": true,
            "properties": {
              "document_id": {
                "type": "string"
              },
              "party_id": {
                "type": "string"
              },
              "type": {
                "type": "string"
              },
              "date": {
                "type": "string",
                "format": "date",
                "example": "2016-09-27"
              },
              "ref": {
                "description": "ref for Document"
              },
              "category": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "sources": {
                "$ref": "https://raw.githubusercontent.com/indiaprotocol/protocol/master/dataTypes/v1.json#/definitions/sources"
              },
              "information": {
                "$ref": "https://raw.githubusercontent.com/indiaprotocol/protocol/master/v1/party/PartyInformations.json"
              }
            }
          }
        }
      }
    }
  }
}