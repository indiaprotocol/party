{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$ref": "#/definitions/Products",
  "definitions": {
    "Products": {
      "type": "object",
      "required": [
        "meta",
        "data"
      ],
      "properties": {
        "meta": {
          "type": "object",
          "required": [
            "page",
            "per_page",
            "total_records",
            "organization_id"
             "sort"
          ],
          "optionals": [
            "filters",
            "q"
          ],
          "properties": {
            "page": {
              "type": "integer"
            },
            "per_page": {
              "type": "integer"
            },
            "total_records": {
              "type": "integer"
            },
            "organization_id": {
              "type": "string"
            },
            "sort": {
              "type": "object",
              "properties": {
                "by": {
                  "type": "string"
                },
                "in": {
                  "type": "string"
                }
              }
            },
            "filters": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string"
                },
                "status": {
                  "type": "string"
                },
                "owner": {
                  "type": "string"
                },
                "order_id": {
                  "type": "string"
                }
              }
            },
            "q": {
              "type": "string"
            }
          }
        },
        "data": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "product_id",
              "order_id",
              "code",
              "type",
              "status",
              "party",
              "owner",
              "organization",
              "created_at",
              "updated_at"
            ],
            "properties": {
              "product_id": {
                "type": "string"
              },
              "order_id": {
                "type": "string"
              },
              "code": {
                "type": "string"
              },
              "type": {
                "type": "string"
              },
              "status": {
                "type": "string"
              },
              "party": {
                "type": "object",
                "required": [
                  "party_id",
                  "name"
                ],
                "properties": {
                  "party_id": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              },
              "owner": {
                "type": "object",
                "required": [
                  "user_id",
                  "name"
                ],
                "properties": {
                  "user_id": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              },
              "organization": {
                "type": "object",
                "required": [
                  "organization_id",
                  "name",
                  "display_name"
                ],
                "properties": {
                  "organization_id": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  },
                  "display_name": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              },
              "created_at": {
                "type": "string"
              },
              "updated_at": {
                "type": "string"
              },
              "party_count": {
                "type": "integer"
              }
            }
          }
        }
      }
    }
  }
}