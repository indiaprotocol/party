{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "definitions": {
    "money": {
      "type": "object",
      "title": "Money",
      "required": [
        "currency",
        "value"
      ],
      "properties": {
        "currency": {
          "default": "INR",
          "description": "ISO 4217 Currency code",
          "title": "Currency",
          "type": "string"
        },
        "value": {
          "description": "Amount in 2 precision float",
          "title": "Value",
          "type": "number",
          "multipleOf": 0.01
        }
      },
      "additionalProperties": false
    },
    "geoCode": {
      "type": "object",
      "required": [
        "lat",
        "lon"
      ],
      "title": "Geo Code",
      "properties": {
        "lon": {
          "default": null,
          "description": "longitude",
          "title": "longitude",
          "type": "number"
        },
        "lat": {
          "default": null,
          "description": "Latitude",
          "title": "Latitude",
          "type": "number"
        }
      },
      "additionalProperties": false
    },
    "dateRange": {
      "type": "object",
      "title": "DateRange",
      "properties": {
        "start": {
          "description": "start Date of range",
          "title": "start",
          "type": "string",
          "format": "date"
        },
        "end": {
          "description": "end Date of ranfe",
          "title": "end",
          "type": "string",
          "format": "date"
        }
      },
      "additionalProperties": false
    },
    "sources": {
      "type": "array",
      "title": "Data sources",
      "description": "The sources reference of the data",
      "items": {
        "type": "object",
        "properties": {
          "ref": {
            "description": "source reference from sources dictionary",
            "type": "string"
          },
          "timestamp": {
            "type": "string",
            "format": "dateTime",
            "description": "timestamp of the last refresh of data from source"
          }
        },
        "additionalProperties": false
      },
      "additionalItems": true
    }
  }
}